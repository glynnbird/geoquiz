{
  "docs": [
    {
        "_id": "_design/fetch",
        "views": {
            "byName": {
                "map": "function(doc) {\n  if (typeof doc.properties =='object') { emit(doc.properties.name, null); }\n}"
            },
            "byType": {
              "map": "function(doc) {\n  emit(doc.type, 1)\n}",
              "reduce": "_count"
            }
        },
        "language": "javascript"
    }
  ]
}
